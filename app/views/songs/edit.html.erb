<h1> Update a Song </h1>

<form action="/artists/<%= @song.artist_id %>/songs/<%= @song.id %>" method="POST">
  <input type="hidden" name="_method" value="patch">
  <input name="authenticity_token" type="hidden" value="<%= form_authenticity_token %>" />
  <label>
    Title:
    <input type="text" name="title" value="<%= @song.title %>" />
  </label>

  <label>
    Year: 
    <input type="integer" name="year" value="<%= @song.year %>"/>
  </label>

  <button>Update your Song</button>
</form>